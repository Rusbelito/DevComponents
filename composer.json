{
    "name": "rusbelito/dev-components",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rusbelito\\DevComponents\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rusbelito\\DevComponents\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Rusbelito"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "laravel/homestead": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rusbelito\\DevComponents\\DevComponentsServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan vendor:publish --provider=\"Rusbelito\\DevComponents\\DevComponentsServiceProvider\"",
            "npm install",
            "npm run build"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --provider=\"Rusbelito\\DevComponents\\DevComponentsServiceProvider\"",
            "npm install",
            "npm run build"
        ]
    }
}
