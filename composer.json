{
    "name": "rusbelito/dev-components",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rusbelito\\DevComponents\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Rusbelito"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {},
    "extra": {
    "laravel": {
        "providers": [
            "Rusbelito\\DevComponents\\DevComponentsServiceProvider"
        ]
    }
},
    "scripts": {
        "post-install-cmd": [
            "php artisan vendor:publish --provider=\"Rusbelito\\DevComponents\\DevComponentsServiceProvider\" --tag=\"vistas\""
        ],
        "post-update-cmd": [
            "php artisan vendor:publish --provider=\"Rusbelito\\DevComponents\\DevComponentsServiceProvider\" --tag=\"vistas\""
        ]
    }

}
